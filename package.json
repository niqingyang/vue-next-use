{
  "name": "vue-next-use",
  "version": "0.0.21-beta",
  "description": "Collection of Vue Hooks",
  "main": "dist/vue-next-use.bundle.esm.js",
  "module": "dist/vue-next-use.bundle.esm.js",
  "type": "module",
  "types": "dist/vue-next-use.d.ts",
  "typings": "dist/vue-next-use.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-next-use.bundle.esm.js",
      "require": "./dist/vue-next-use.bundle.cjs.js"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build:cjs": "tsc -d --sourceMap",
    "build:es": "tsc -d --sourceMap -m esNext --outDir esm",
    "build": "rollup -c --sourcemap",
    "clean": "rimraf node_modules"
  },
  "author": "niqingyang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/niqingyang/vue-next-use"
  },
  "bugs": {
    "url": "https://github.com/niqingyang/vue-next-use/issues"
  },
  "homepage": "https://github.com/niqingyang/vue-next-use#readme",
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "js-cookie": "^2.2.1",
    "rebound": "^0.1.0",
    "screenfull": "^5.1.0",
    "set-harmonic-interval": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/node": "^14.14.30",
    "@vue/compiler-sfc": "^3.0.7",
    "rollup": "^2.40.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5",
    "vue": "3.0.7"
  }
}
