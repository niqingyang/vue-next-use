{
  "name": "vue-next-use",
  "version": "0.0.7-beta",
  "description": "Collection of Vue Hooks",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "src/index.ts",
  "typings": "src/index.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./esm/index.js"
    }
  },
  "files": [
    "lib/",
    "esm/",
    "src/"
  ],
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "npm run build:cjs && npm run build:es",
    "clean": "rimraf node_modules"
  },
  "author": "niqingyang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/niqingyang/vue-next-use"
  },
  "bugs": {
    "url": "https://github.com/niqingyang/vue-next-use/issues"
  },
  "homepage": "https://github.com/niqingyang/vue-next-use#readme",
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.30",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "typescript": "^4.1.5",
    "vite": "^2.0.1"
  }
}
